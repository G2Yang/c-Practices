        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaWpf"
        mc:Ignorable="d"
        Title="ApartadoD" Height="450" Width="800">
    <StackPanel Orientation="Vertical" HorizontalAlignment="Left" Margin="0,29,0,39">
        <DockPanel Width="275" Height="57" >
            <Label Name="Etiqueta1" Target="{Binding ElementName=Texto1}"
VerticalAlignment="Center">
